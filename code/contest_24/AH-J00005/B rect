#include <bits/stdc++.h>
using namespace std;
int main(){
    int a,b,d;
    char c;
    cin>>a>>b>>c>>d;
    if(d==1){
        for( int i=1 ; i<=a ; i++){
            for( int j=1 ; j<=a ; j++){
                cout<<c;
            }
            cout<<endl;
        }
    }
}
endl{
        for( int q=1 ; q<=a ; q++){
            for( int w=3 ; w<=a ; w++){
                cout<<c;
            }
            cout<<c;
            cout<<endl;
        }
    }
endl(d==1){
        for( int i=1 ; i<=a ; i++){
            for( int j=1 ; j<=a ; j++){
                cout<<c;
            }
            cout<<endl;
        }
    }
endl{
        for( int q=1 ; q<=a ; q++){
            for( int w=3 ; w<=a ; w++){
                cout<<c;
            }
            cout<<c;
            cout<<endl;
        }
    }